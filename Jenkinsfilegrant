pipeline{
    agent any
    stages{
        stage('Terragrunt Apply'){
            steps{
                withAWS(credentials: 'aws-credential-geocitizen') {
                    sh 'echo $AWS_SECRET_ACCESS_KEY'
                     sh 'echo $AWS_ACCESS_KEY_ID'
                    sh '(cd aws-geo && terragrunt apply --auto-approve)'
                }
            }
        }
    }
    post {
        success {
            echo 'all good!!!!'
        }
        failure { 
            echo 'something went wrong'
        }
    }
}